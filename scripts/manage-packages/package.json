{
    "name": "manage_packages",
    "version": "1.0.0",
    "description": "Script for managing images stored in GHCR.io",
    "main": "index.ts",
    "engines": {
        "node": ">=22.18",
        "pnpm": ">=10"
    },
    "devEngines": {
        "packageManager": {
            "name": "pnpm"
        }
    },
    "scripts": {
        "build": "echo Compiling... && tsc",
        "build:dev": "tsc --watch",
        "lint": "eslint --fix src && node --run prettify",
        "prettify": "prettier --write '{src,test}/**/*.{js,ts}' '*.{js,json}'",
        "start": "node dist/index.js",
        "start:dev": "tsx src/index.ts",
        "test": "node --run build && node --test test/**/*.test.js",
        "typecheck": "tsc --noEmit"
    },
    "keywords": [],
    "author": "Paulo Ferreira de Castro (https://github.com/pdcastro)",
    "type": "module",
    "dependencies": {
        "got": "^14.4.7",
        "@octokit/auth-action": "^6.0.1",
        "@octokit/types": "^14.1.0",
        "octokit": "^5.0.3",
        "p-limit": "^6.2.0",
        "p-throttle": "^7.0.0",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.33.0",
        "@types/node": "^24.2.1",
        "eslint": "^9.33.0",
        "prettier": "^3.6.2",
        "tsx": "^4.20.3",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.39.0"
    }
}
